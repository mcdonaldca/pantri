<div id="pantry">
  <div class="stripe"></div>
  <div class="stripe right"></div>

  <% @groups.each do |group| %>
    <div class="shelf <%= group %>">
      <svg version="1.1" x="0px" y="0px" width="605px" height="32px" viewBox="0 0 605 32" enable-background="new 0 0 605 32" xml:space="preserve">
        <% @dot_x.each do |x| %>
          <circle fill="#EA6044" cx="<%= x %>" cy="16" r="7.9"/>
        <% end %>
      </svg>
    </div>
  <% end %>
</div>